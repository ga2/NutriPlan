<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/dim_margin_high">

    <LinearLayout
        android:id="@+id/dialogimportexport_linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView4"
                style="@style/style_form_label"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/export_nutr" />

            <TextView
                android:id="@+id/textView7"
                style="@style/style_textView_form_options"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dim_margin_normal"
                android:text="@string/export_description" />
        </LinearLayout>

        <ImageButton
            android:id="@+id/dialogimportexport_imageView_export"
            android:layout_width="196dp"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/export_nutr" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/dialogimportexport_linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dialogimportexport_linearLayout1">

        <View
            android:id="@+id/view2"
            style="@style/style_divider"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_weight="1"
            app:layout_constraintBottom_toBottomOf="@+id/dialogimportexport_linearLayout1"
            app:layout_constraintEnd_toEndOf="parent" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/dialogimportexport_linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:paddingBottom="@dimen/dim_margin_highest"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dialogimportexport_linearLayout2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView5"
                style="@style/style_form_label"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/import_nutr" />

            <TextView
                android:id="@+id/textView8"
                style="@style/style_textView_form_options"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dim_margin_normal"
                android:text="@string/import_description" />
        </LinearLayout>

        <ImageButton
            android:id="@+id/dialogimportexport_imageView_import"
            android:layout_width="196dp"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/import_nutr" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>